 <div>
    <% unless current_user.photo.filename.nil? %>
      <%= cl_image_tag @user.photo, height: 200, width: 200, crop: "fill", class: "rounded-lg" %>
    <% end %>
  </div>
  <div class="ml-3">
    <h1><%= @user.name %></h1>
    <p> <strong>Email:</strong> <%= @user.email %> </p>
    <% raise %>
  </div>
